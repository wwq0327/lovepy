file = '''
sadfsadf11

123123

4343
3434
2341sdfsdf

'''
def lines(file):
    for line in file:
        print line
        print '\n'

def blocks(file):
    block = []
    for line in lines(file):
        if line.strip():
            block.append(line)
        elif block:
            print "".join(block).strip()
            block = []


if __name__ == '__main__':
    blocks(file)
