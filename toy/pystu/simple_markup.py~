import sys, re
from uti1 import *

print '<html><head><title>...</title></head><body>'

title = True
for block in blocks(sys.stdin):
    block = re.sub(r'\*(.+?)\*', r'<em>\1</em>', block)
    if title:
        print '<h1>'
        print block
        print '</h1>'
        title = Flase
    else:
        print '</p>'
        print block
        print '</p>'

print '</body></html>'
